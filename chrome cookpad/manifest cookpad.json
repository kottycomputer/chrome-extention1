{
    "name": "Cookpad Chrome Extension",
    "description": "Remove Unnecessary sugar, cheese and chocolate from cookpad etc.",
    "version": "0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://cookpad.com/*"],
          "js": ["content_script cookpad.js"]
        },
        {
          "matches": ["https://cookpad.com/*"],
          "css": ["mystyle2.css"]
        }
      ]
  }
  

  